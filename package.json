{
  "name": "redux-ng",
  "version": "0.0.1",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --aot",
    "hmr": "ng serve --hmr -e=hmr",
    "preserve": "NODE_ENV=production yarn build-prod",
    "serve": "cd dist && live-server --port=8090",
    "build": "ng build --sourcemaps",
    "prebuild-prod": "ng -v",
    "build-prod": "ng build --prod --sourcemaps --aot --vc --base-href /spa --deploy-url /angular-deploy-path",
    "build-for-apache": "NODE_ENV=production yarn build-prod && mv ./dist/ ../dist",
    "start-c9": "NODE_ENV=production ng serve --host $IP --port $PORT --public $C9_HOSTNAME",
    "c9-apache-restart": "sudo /etc/init.d/apache2 restart",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build-stat": "ng build --prod --aot --stats-json",
    "bundle-report": "webpack-bundle-analyzer dist/stats.json",
    "analyze": "ng build --prod --aot --stats-json && webpack-bundle-analyzer dist/stats.json",
    "release": "standard-version"
  },
  "private": true,
  "dependencies": {
    "@angular-redux/router": "^7.0.0",
    "@angular-redux/store": "^7.0.2",
    "@angular/animations": "5.2.6",
    "@angular/cdk": "^5.0.1",
    "@angular/common": "5.2.6",
    "@angular/compiler": "5.2.6",
    "@angular/core": "5.2.6",
    "@angular/flex-layout": "^5.0.0-beta.13",
    "@angular/forms": "5.2.6",
    "@angular/http": "5.2.6",
    "@angular/material": "5.2.3",
    "@angular/platform-browser": "5.2.6",
    "@angular/platform-browser-dynamic": "5.2.6",
    "@angular/router": "5.2.6",
    "@angularclass/hmr": "^2.1.3",
    "bulma": "^0.6.2",
    "bulmaswatch": "^0.6.2",
    "core-js": "^2.4.1",
    "font-awesome": "^4.7.0",
    "hammerjs": "^2.0.8",
    "normalize.css": "^7.0.0",
    "redux": "^3.7.2",
    "redux-logger": "^3.0.6",
    "redux-observable": "0.18.0",
    "redux-undo": "^1.0.0-beta9-9-7",
    "rxjs": "^5.5.6",
    "showdown": "^1.8.6",
    "stacktrace-js": "^2.0.0",
    "tslib": "^1.8.1",
    "webpack-bundle-analyzer": "^2.9.0",
    "zone.js": "^0.8.20"
  },
  "devDependencies": {
    "@angular/cli": "1.7.1",
    "@angular/compiler-cli": "5.2.6",
    "@angular/language-service": "5.2.6",
    "@types/jasmine": "~2.5.53",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "~6.0.60",
    "@types/redux-logger": "^3.0.4",
    "codelyzer": "^4.0.2",
    "jasmine-core": "~2.6.2",
    "jasmine-spec-reporter": "~4.1.0",
    "karma": "~1.7.0",
    "karma-chrome-launcher": "~2.1.1",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "live-server": "^1.2.0",
    "protractor": "~5.1.2",
    "source-map-explorer": "^1.5.0",
    "standard-version": "^4.3.0",
    "ts-node": "~3.2.0",
    "tslint": "~5.3.2",
    "typescript": "2.6.2"
  }
}
